<ScrubberConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ScrubberConfiguration.xsd">
	<ScrubberImplementation>XML_TEXT</ScrubberImplementation>
	<TextProcessingRules>
		<SavedIdentifiers>
			<Label>Names</Label>
			<FileLocation>conf/names.txt</FileLocation>
			<ReplaceWith>xxxNAMExxx</ReplaceWith>
			<CaseSensitive>false</CaseSensitive>
		</SavedIdentifiers>
		<SavedIdentifiers>
			<Label>PrivateDictionary</Label>
			<FileLocation>conf/private_dict.txt</FileLocation>
			<ReplaceWith>xxxPRIVATExxx</ReplaceWith>
			<CaseSensitive>false</CaseSensitive>
		</SavedIdentifiers>
		<SavedIdentifiers>
			<Label>Institutions</Label>
			<FileLocation>conf/hospital_names.txt</FileLocation>
			<ReplaceWith>xxxHOSPITALxxx</ReplaceWith>
			<CaseSensitive>false</CaseSensitive>
		</SavedIdentifiers>	
		<RegularExpressions>
			<FileLocation>conf/regex.txt</FileLocation>
			<ReplaceWith>xxxx</ReplaceWith>
			<PrintRegexNameInResult>true</PrintRegexNameInResult>
		</RegularExpressions>
	</TextProcessingRules>
	<XMLTextProcessingRules>
		<Validation>
			<EnableValidation>false</EnableValidation>
			<SchemaLocation/>
		</Validation>
		<IdentifyingRegions>
			<Region>
				<Label>Identifers</Label>
				<XPATH>//Identifiers</XPATH>
				<ReplaceWith>xxxIDENTIFIERxxx</ReplaceWith>
				<CaseSensitive>false</CaseSensitive>
			</Region>			
		</IdentifyingRegions>
		<RegionsToScrub>
			<Region>
				<Label>DEFAULT_SCRUB_ALL</Label>
				<XPATH>/*</XPATH>
			</Region>
		</RegionsToScrub>
	</XMLTextProcessingRules>
	<Recorder>
		<Implementation>DEFAULT</Implementation>
		<EnableRecording>true</EnableRecording>
		<ConfigParam key="Text" value="Text"/>
	</Recorder>
</ScrubberConfiguration>
