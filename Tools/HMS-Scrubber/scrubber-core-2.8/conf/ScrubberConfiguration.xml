<ScrubberConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ScrubberConfiguration.xsd">
	<ScrubberImplementation>XML_TEXT</ScrubberImplementation>
	<EnablePassthrough>false</EnablePassthrough>
	<TextProcessingRules>
		<SavedIdentifiers>
			<Label>Names</Label>
			<FileLocation>conf/names.txt</FileLocation>
			<ReplaceWith>xxxNAMExxx</ReplaceWith>
			<CaseSensitive>false</CaseSensitive>
		</SavedIdentifiers>
		<SavedIdentifiers>
			<Label>Institutions</Label>
			<FileLocation>conf/hospital_names.txt</FileLocation>
			<ReplaceWith>xxxHOSPITALxxx</ReplaceWith>
			<CaseSensitive>false</CaseSensitive>
		</SavedIdentifiers>
		<SavedIdentifiers>
			<Label>Institutions</Label>
			<FileLocation>conf/positionDictionary/positionDictionary50.txt</FileLocation>
			<ReplaceWith>xxxPOSITIONxxx</ReplaceWith>
			<CaseSensitive>false</CaseSensitive>
		</SavedIdentifiers>
		<SavedIdentifiers>
			<Label>PrivateDictionary</Label>
			<FileLocation>conf/private_dict.txt</FileLocation>
			<ReplaceWith>xxxxPRIVATExxx</ReplaceWith>
			<CaseSensitive>false</CaseSensitive>
		</SavedIdentifiers>
		<RegularExpressions>
			<FileLocation>conf/regex.txt</FileLocation>
			<ReplaceWith>xxxx</ReplaceWith>
			<PrintRegexNameInResult>true</PrintRegexNameInResult>
		</RegularExpressions>
	</TextProcessingRules>
	<XMLTextProcessingRules>
		<Validation>
			<EnableValidation>true</EnableValidation>
			<SchemaLocation/>
		</Validation>
		<IdentifyingRegions>
			<Region>
				<Label>FirstName</Label>
				<XPATH>/Envelope/Header/Identifiers/FirstName</XPATH>
				<ReplaceWith>xxxFirstNamexxx</ReplaceWith>
				<CaseSensitive>false</CaseSensitive>
			</Region>
			<Region>
				<Label>LastName</Label>
				<XPATH>/Envelope/Header/Identifiers/LastName</XPATH>
				<ReplaceWith>xxxLastNamexxx</ReplaceWith>
				<CaseSensitive>false</CaseSensitive>
			</Region>
			<Region>
				<Label>DateOfBirth</Label>
				<XPATH>/Envelope/Header/Identifiers/DateOfBirth</XPATH>
				<ReplaceWith>xxxDateOfBirthxxx</ReplaceWith>
				<CaseSensitive>false</CaseSensitive>
			</Region>
			<Region>
				<Label>AccessionNumber</Label>
				<XPATH>/Envelope/Header/Identifiers/AccessionNumber</XPATH>
				<ReplaceWith>xxxAccessionNumberxxx</ReplaceWith>
				<CaseSensitive>false</CaseSensitive>
			</Region>
			<Region>
				<Label>LocalMRN</Label>
				<XPATH>/Envelope/Header/Identifiers/LocalMRN</XPATH>
				<ReplaceWith>xxxLocalMRNxxx</ReplaceWith>
				<CaseSensitive>false</CaseSensitive>
			</Region>
			<Region>
				<Label>TissueAcquisitionDate</Label>
				<XPATH>/Envelope/Body/PathologyCase/@TissueAcquisitionDate</XPATH>
				<ReplaceWith>xxxDATExxx</ReplaceWith>
				<CaseSensitive>false</CaseSensitive>
			</Region>
			<Region>
				<Label>ReportDate</Label>
				<XPATH>/Envelope/Body/PathologyCase/FullReportData/@ReportDate</XPATH>
				<ReplaceWith>xxxDATExxx</ReplaceWith>
				<CaseSensitive>false</CaseSensitive>
			</Region>
			
			
		</IdentifyingRegions>
		<RegionsToScrub>
			<Region>
				<Label>ClinicalInformation</Label>
				<XPATH>/Envelope/Body/PathologyCase/Clinical/ClinicalInformation</XPATH>
			</Region>
			<Region>
				<Label>SpecimenGrossDescription</Label>
				<XPATH>/Envelope/Body/PathologyCase/Specimen/GrossDescription/Text</XPATH>
			</Region>
			<Region>
				<Label>GrossDescription</Label>
				<XPATH>/Envelope/Body/PathologyCase/GrossDescriptionText</XPATH>
			</Region>
			<Region>
				<Label>DiagnosisText</Label>
				<XPATH>/Envelope/Body/PathologyCase/DiagnosisText</XPATH>
			</Region>
			<Region>
				<Label>FullReportText</Label>
				<XPATH>/Envelope/Body/PathologyCase/FullReportText</XPATH>
			</Region>
			<Region>
				<Label>FullReportData</Label>
				<XPATH>/Envelope/Body/PathologyCase/FullReportData
				</XPATH>
			</Region>
			
		</RegionsToScrub>
	</XMLTextProcessingRules>
	<Recorder>
		<Implementation>DEFAULT</Implementation>
		<EnableRecording>true</EnableRecording>
		<ConfigParam key="Text" value="Text"/>
		<ConfigParam key="DRIVER" value="com.mysql.jdbc.Driver"/>
		<ConfigParam key="DB_USER" value="root"/>
		<ConfigParam key="DB_PW" value="root"/>
		<ConfigParam key="JDBC_URL" value="jdbc:mysql://localhost:3306/scrubber?autoReconnect=true"/>
	</Recorder>
</ScrubberConfiguration>